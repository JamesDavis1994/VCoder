{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "mp4",
  "name": "MPEG-4 (.mp4)",
  "extension": "mp4",
  "encoders": {
    "video": [ "h264_amf", "h264_nvenc", "hevc_amf", "hevc_nvenc", "libx264", "libx265" ],
    "audio": [ "aac", "libfdk_aac", "pcm_s16le", "pcm_s24le", "pcm_s32le" ]
  },
  "capabilities": {
    "faststart": true
  }
}
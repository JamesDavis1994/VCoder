            "items": [
              {
                "value": "veryfast"
              },
              {
                "value": "faster"
              },
              {
                "value": "fast"
              },
              {
                "value": "medium"
              },
              {
                "value": "slow"
              },
              {
                "value": "slower"
              },
              {
                "value": "veryslow"
              }
            ]
          }
        }
      ]
    }
  ]
}